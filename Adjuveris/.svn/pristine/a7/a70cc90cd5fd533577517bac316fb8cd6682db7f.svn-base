package com.linguaclassica.model;

import java.io.Serializable;

/**
 * Business layer interface for user permissions model corresponding 
 * to the user_permission look-up table.
 * @author Cris
 *
 */
public interface UserPermissionModel extends Serializable
{
	/**
	 * Gets the id
	 * @return
	 */
	public Integer getId();
	
	/**
	 * Sets the id
	 * @param id
	 */
	public void setId(Integer id);
	
	/**
	 * Returns the user ID
	 * @return
	 */
	public Integer getUserId();
	
	/**
	 * Returns the user permission id
	 * @return
	 */	
	public Integer getPermissionId();	

	/**
	 * Sets the user id for the corresponding permission
	 * @param id
	 */
	public void setUserId(Integer id);
	
	/**
	 * Sets the permission id for the corresponding user
	 * @param id
	 */
	public void setPermissionId(Integer id);
}
