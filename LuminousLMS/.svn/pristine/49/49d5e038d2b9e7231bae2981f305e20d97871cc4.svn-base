package com.linguaclassica.model;

import java.io.Serializable;
import java.sql.Blob;

public interface ResourceModel extends Serializable {

	Integer getId();

	String getName();
	
	/**
	 * Sets the name
	 * @param name
	 */
	void setName(String name);

	Blob getResource();
	
	/**
	 * Sets the resource blob
	 * @param resource
	 */
	void setResource(Blob resource);

	String getExt();
	
	/**
	 * Sets the extension
	 * @param ext
	 */
	void setExt(String ext);

}
